	Isbn = {978-1-58113-702-6},
	Keywords = {distributed knowledge acquisition, image labeling, online games, World Wide Web},
	Pages = {319--326},
	Publisher = {ACM},
	Series = {{CHI} '04},
	Title = {Labeling {Images} with a {Computer} {Game}},
