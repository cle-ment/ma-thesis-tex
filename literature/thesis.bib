	Journal = {IEEE Transactions on Neural Networks},
	Keywords = {Computer architecture, conditional distributions, Context modeling, Data mining, dependency tests, dimensionality, graphical model, Graphical models, graph theory, high-dimensional discrete data, Intelligent networks, joint distributions, multilayer neural network, Multi-layer neural network, multilayer perceptrons, network pruning, Neural networks, probability, Random variables, Smoothing methods, Testing},
	Month = may,
