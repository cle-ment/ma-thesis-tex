	Keywords = {automatic human language analysis, automatic human language representation, batch processing, business management, computational techniques, Google, Internet, jumping NLP curves, Knowledge based systems, marketing prediction, natural language processing, natural language processing research, natural language understanding, NLP research evolution, NLP technology, Pragmatics, pragmatics curve, punch cards, search engines, Semantics, semantics curve, Syntactics, syntactics curve, Webpages},
